<Window x:Class="Lab6.UI.SprintReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab6.UI"
        mc:Ignorable="d"
        Title="SprintReport" Height="300" Width="400"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="Create" FontSize="20" Content="Создать" Margin="94,205,94,16" Click="Save_Click"/>
        <Label Content="Введите id сотрудника" FontSize="20" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="36" Width="225"/>
        <TextBox x:Name="EId" HorizontalAlignment="Left" Margin="258,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Введите описание отчета" FontSize="20" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top" Height="38" Width="248"/>
        <TextBox x:Name="TDisc" HorizontalAlignment="Left" Margin="258,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
